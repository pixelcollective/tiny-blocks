{
  "name": "tiny-pixel/blocks",
  "type": "library",
  "version": "1.0.0",
  "description": "For consummate blockbuilders",
  "license": "MIT",
  "authors": [
    {
      "name": "Kelly Mears",
      "homepage": "https://tinypixel.dev"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1",
    "composer/installers": "v2",
    "illuminate/collections": "^8.61",
    "eftec/bladeone": "^3.24",
    "php-di/php-di": "^6.0"
  },
  "require-dev": {
    "roots/wordpress": "dev-master"
  },
  "autoload": {
    "files": [
      "vendor/illuminate/collections/src/Collect/Support/helpers.php",
      "vendor/illuminate/collections/src/Collect/Support/alias.php"
    ],
    "psr-4": {
      "TinyBlocks\\": [
        "./",
        "./src",
        "./demo"
      ]
    }
  },
  "extra": {
    "wordpress-install-dir": "wordpress"
  },
  "scripts": {
    "lint": [
      "phpcs --ignore=vendor,demo --extensions=php --standard=PSR12 ."
    ]
  }
}
